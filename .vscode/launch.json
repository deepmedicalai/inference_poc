    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main/manage.py",
            "cwd": "${workspaceFolder}/main/",
            "stopOnEntry": true,
            "env": {
                "FLASK_APP": "webserver",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "1",
                "APP_SETTINGS": "settings.config.DevelopmentConfig"
            },
            "args": [
                "run",
                "-h",
                "0.0.0.0"
            ],
            "jinja": true
        },
        {
            "name": "Python: Worker",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main/manage.py",
            "cwd": "${workspaceFolder}/main/",
            "stopOnEntry": true,
            "env": {
                "FLASK_APP": "webserver",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "1",
                "APP_SETTINGS": "settings.config.DevelopmentConfig"
            },
            "args": [
                "run_worker"
            ],
            "jinja": true
        },
       
    ]
}